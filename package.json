{
	"name": "megami",
	"version": "0.1.0",
	"description": "Modern imageboard engine with Nest.js",
	"author": "d-indifference <umaruchan825@gmail.com>",
	"private": true,
	"license": "GPLV2",
	"scripts": {
		"css-once": "node_modules/node-sass/bin/node-sass public/scss/megami.scss public/css/megami.css",
		"css-watch": "npm run css-once && node_modules/node-sass/bin/node-sass public/scss/megami.scss public/css/megami.css --watch",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "npm run css-watch && nest start",
		"start:dev": "concurrently --kill-others \"npm run css-watch\" \"nest start --watch\"",
		"start:debug": "concurrently --kill-others \"npm run css-watch\" \"nest start --debug --watch\"",
		"start:prod": "npm run css-once && node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prisma:migrate": "prisma migrate dev"
	},
	"dependencies": {
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.0.0",
		"@nestjs/core": "^10.0.0",
		"@nestjs/platform-express": "^10.0.0",
		"@prisma/client": "^4.16.2",
		"@types/he": "^1.2.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cookie-parser": "^1.4.6",
		"crypto-js": "^4.1.1",
		"dotenv": "^16.3.1",
		"express-handlebars": "^7.0.7",
		"express-session": "^1.17.3",
		"filesize": "^10.0.12",
		"fs-extra": "^11.1.1",
		"hbs": "^4.2.0",
		"he": "^1.2.0",
		"luxon": "^3.3.0",
		"nestjs-form-data": "^1.9.0",
		"nestjs-ip-filter": "^0.2.0",
		"nestjs-real-ip": "^3.0.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"sass": "^1.63.6",
		"uuidv4": "^6.2.13"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/crypto-js": "^4.1.1",
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.17.7",
		"@types/fs-extra": "^11.0.1",
		"@types/hbs": "^4.0.1",
		"@types/jest": "^29.5.2",
		"@types/luxon": "^3.3.0",
		"@types/multer": "^1.4.7",
		"@types/node": "^20.3.1",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"concurrently": "^8.2.0",
		"dotenv-cli": "^7.2.1",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"node-sass": "^9.0.0",
		"prettier": "^2.8.8",
		"prisma": "^4.16.2",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	}
}
